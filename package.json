{
  "private": true,
  "packageManager": "pnpm@8.6.0",
  "scripts": {
    "build": "vite build",
    "dev": "vite --port 3333 --open",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "up": "taze major -I",
    "postinstall": "npx simple-git-hooks"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.16.0",
    "react-use": "^17.4.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.39.8",
    "@iconify-json/carbon": "^1.1.21",
    "@testing-library/react": "^14.0.0",
    "@types/node": "^20.6.0",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@unocss/eslint-config": "^0.53.6",
    "@unocss/reset": "^0.53.6",
    "@vitejs/plugin-react": "^4.0.4",
    "eslint": "^8.49.0",
    "jsdom": "^22.1.0",
    "lint-staged": "^13.3.0",
    "pnpm": "^8.7.5",
    "simple-git-hooks": "^2.9.0",
    "taze": "^0.10.3",
    "typescript": "^5.2.2",
    "unocss": "^0.53.6",
    "unplugin-auto-import": "^0.16.6",
    "usehooks-ts": "^2.9.1",
    "vite": "^4.4.9",
    "vite-plugin-pages": "^0.31.0",
    "vitest": "^0.31.4"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  },
  "eslintConfig": {
    "extends": [
      "@antfu",
      "@unocss"
    ]
  }
}
